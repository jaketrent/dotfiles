{
  "workbench.activityBar.visible": false,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "explorer.openEditors.visible": 0,
  "editor.codeLens": true,
  "workbench.editor.showTabs": false,
  "breadcrumbs.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.startupEditor": "newUntitledFile",
  "editor.accessibilitySupport": "off",
  "extensions.ignoreRecommendations": true,
  "editor.fontSize": 16,
  "eslint.alwaysShowStatus": true,
  "editor.formatOnSave": true,
  "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
  "terminal.integrated.fontSize": 16,
  "liveshare.featureSet": "stable",
  "vscode-neovim.highlightGroups.unknownHighlight": {},
  "vscode-neovim.neovimInitPath": "~/.config/nvim/init.vim",
  "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
  "vscode-neovim.useCtrlKeysForNormalMode": true,
  "vscode-neovim.useCtrlKeysForInsertMode": true
}